{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": { "path": "Logs/modelchallengebot.txt" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Destructure": [
      {
        "Name": "ToMaximumDepth",
        "Args": { "maximumDestructuringDepth": 4 }
      },
      {
        "Name": "ToMaximumStringLength",
        "Args": { "maximumStringLength": 100 }
      },
      {
        "Name": "ToMaximumCollectionCount",
        "Args": { "maximumCollectionCount": 10 }
      }
    ],
    "Properties": {
      "Application": "ModelChallengeBot"
    }
  },
  "Workspace": {
    "Directory": "",
    "RenderFolder": "Render",
    "DownloadFolder": "Downloads"
  },
  "Discord": {
    "CommandPrefix": "!",
    "Secret": "",
    "Admin": 0,
    "Guild": 0
  },
  "Challenge": {
    "MaxTimeLimitHours": 99,
    "MaxDurationDays": 99,
    "TimeLeftWarningMinutes": 5,
    "CharacterLimit": 200
  },
  "Blender": {
    "FileTemplatePath": "",
    "Path": "",
    "RenderArgs": "-b {0} -E BLENDER_EEVEE -s 1 -e 33 -o {1} -a", // expected arguments 0(blend file): ""C:\Users\videotest.blend"" 1(file output): x:/videotest/1/output
    "OutputSuffix": ""
  },
  "Database": {
    "fileName": "",
    "ConnectionString": ""
  }
}